{
  "suggest.enablePreview": true,
  "javascript.suggest.enabled": false,
  "tsserver.enableJavascript": true,
  "diagnostic.virtualText": false,
  "coc.source.emoji.filetypes": null,
  "yaml.schemas": {
    "http://json.schemastore.org/gitlab-ci": ".gitlab-*.yml"
  },
  "json.schemas": [
    {
      "name": "tsconfig.json",
      "description": "TypeScript compiler configuration file",
      "fileMatch": ["tsconfig.*json"],
      "url": "http://json.schemastore.org/tsconfig"
    }
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "yaml",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "typescript.tsx",
    "json"
  ],
  "prettier.ignorePath": "/Users/antonvasin/.prettierignore",
  "typescript.showUnused": false,
  "javascript.showUnused": false,
  "git.gitlab.hosts": [
    "gitlab.com"
  ],
  "tsserver.log": "normal",
  "javascript.updateImportsOnFileMove.enable": false,
  "snippets.extends": {
    "typescript": ["javascript"]
  },
  "go.goplsOptions": {
    "gofumpt": true
  },
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck",
    "javascript": "eslint",
    "typescript": "eslint"
  }
}
